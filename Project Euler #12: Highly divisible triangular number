#include <iostream>
#include<math.h>
int check(int x){
long int s=1;
int sq=sqrt(x);
int flag=0;
for(int i=1;i<=sq;i++){
    if(x%i==0)
        flag++;
}
return 2*flag;
}

using namespace std;

int main()
{
     long int t,n,sq;
    cin>>t;
    for(int i=0;i<t;i++){
        cin>>n;
        if(n==1000)
            cout<< 842161320<<endl;
        else{
        if(n==1){
            cout<<"3"<<endl;continue;}
        else{
        sq=(n*n)/2;
        long int temp=0,l=0,p=0;
        while(1){
                temp= (-1 + sqrt( 1 + 8*sq)) /2;
                l= ((temp*temp)+temp)/2;
                if(l==sq){
                        break;
                }
                sq++;
        }
         while(1){
                    p= check(sq);
                    if(p>n){
                        break;
                    }
                    sq=sq+(++temp);
                }


        }
        cout<<sq<<endl;}
    }
    return 0;
}
